
    <h1 align="center"> Listes des Materiels</h1>

    <table class="table">
        <thead>
        <tr>
            {% block body %}

            <th>Nom</th>
            <th>Marque</th>
            <th>Quantite</th>


            <th>Categorie</th>
            <th>Etat</th>
        </tr>
        </thead>
        <tbody id='hi'>
        {% for materiel in materiel %}
            <tr>

                <td>{{ materiel.nom }}</td>
                <td>{{ materiel.marque }}</td>
                <td>{{ materiel.quantite }}</td>

                <td>{{ materiel.idcategorie }}</td>
                <td>{{ materiel.idetat }}</td>
                <td>

                </td>
            </tr>

        {% endfor %}
        </tbody>
    </table>



{% endblock %}